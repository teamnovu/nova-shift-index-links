{
    "name": "pavloniym/nova-shift-index-links",
    "description": "A Laravel Nova field to move index links to custom position",
    "keywords": [
        "laravel",
        "nova",
        "index links",
        "shift index links"
    ],
    "license": "MIT",
    "require": {
        "php": ">=8.0",
        "laravel/nova": "^4.1|^5.0"
    },
    "autoload": {
        "psr-4": {
            "Pavloniym\\ShiftIndexLinks\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Pavloniym\\ShiftIndexLinks\\FieldServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
